    "pinia": "2.1.7",
    "query-string": "^8.1.0",
    "style-resources-loader": "^1.5.0",
    "vue": "^3.3.7",
    "vue-eslint-parser": "^9.3.2",
    "vue-i18n": "^9.6.2",
